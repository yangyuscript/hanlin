<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hanlin.domain.CourseType" table="t_courseType">
		<id name="courseTypeId" column="courseTypeId" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="courseTypeName" type="java.lang.String">
			<column name="courseTypeName"></column>
		</property>
	</class>
</hibernate-mapping>